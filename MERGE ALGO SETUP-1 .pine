Multicalc(customperiod) =>
    m = 1
    s = 1
    // chartperiod = timeframe.period
    // isintraday = timeframe.isintraday
    if customperiod == '1D' or customperiod == 'D'
        m := 24
        m
    if customperiod == '1W' or customperiod == 'W'
        m := 24 * 7
        m
    if customperiod == '1M' or customperiod == 'M'
        if month == 1 or month == 3 or month == 5 or month == 7 or month == 8 or month == 10 or month == 12
            m := 24 * 7 * 31
            m
        if month == 2
            m := 24 * 7 * 28
            m
        else
            m := 24 * 7 * 30
            m
    else
        if customperiod == '1'
            s := 1
            m := s / 60
            m
        if customperiod == '3'
            s := 3
            m := s / 60
            m
        if customperiod == '5'
            s := 5
            m := s / 60
            m
        if customperiod == '15'
            s := 15
            m := s / 60
            m
        if customperiod == '30'
            s := 30
            m := s / 60
            m
        if customperiod == '60' or customperiod == ''
            s := 60
            m := s / 60
            m
        if customperiod == '120'
            s := 120
            m := s / 60
            m
        if customperiod == '180'
            s := 180
            m := s / 60
            m
        if customperiod == '240'
            s := 240
            m := s / 60
            m
    m

